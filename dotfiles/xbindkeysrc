"pactl set-sink-volume @DEFAULT_SINK@ +1000"
  XF86AudioRaiseVolume

"pactl set-sink-volume @DEFAULT_SINK@ -1000"
  XF86AudioLowerVolume

"pactl set-sink-mute @DEFAULT_SINK@ toggle"
  XF86AudioMute

"playerctl play-pause"
  XF86AudioPlay

"playerctl play-pause"
  Control+Alt + Tab

"playerctl next"
  XF86AudioNext

"playerctl previous"
  XF86AudioPrev

