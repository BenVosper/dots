SCRIPT_DIR=/home/benv/scripts/i3blocks

[nowplaying]
command=$SCRIPT_DIR/nowplaying
interval=30
signal=11
markup=pango

[volume]
command=$SCRIPT_DIR/volume
interval=30
signal=10

[ethernet]
command=$SCRIPT_DIR/ethernet
interval=60

[wifi]
command=$SCRIPT_DIR/ssid
interval=60

[battery]
command=$SCRIPT_DIR/battery
interval=10
markup=pango

[date]
command=date +"%d-%m-%Y %H:%M:%S"
interval=5
